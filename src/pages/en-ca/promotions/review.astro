---
import LandingPage from "@components/pages/promotions/landingPage.astro";
import { lifeInsuranceFAQ } from "strings/faqs";
import PhoneGif from "@assets/landingPage/4-review@2x.png";
import QRCode from "@assets/landingPage/QRCodes/4-qrReview.png";

const strings = {
    title: "Rated 4.8 out of 5 by Goose Customers.",
    paragraph: "Get insurance in 60 seconds!",
    bullets: null,
    QRAlt: "Qr code to scan",
    iconTitle: "Life Insurance",
    QR: QRCode,
    downloadLink: "https://gooseinsurance.onelink.me/FVvL/81803cd4",
};
---

<LandingPage
    strings={strings}
    heroImage={PhoneGif}
    faqStrings={lifeInsuranceFAQ}
    productGridOmit={["Life Insurance"]}
/>
